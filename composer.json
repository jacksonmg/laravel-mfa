{
    "name": "jacksonmg/laravel-mfa",
    "type": "library",
    "description": "A Laravel package of Multi-factor Authentication (MFA/2FA) with a middleware.",
    "keywords": [
        "laravel",
        "multi-factor",
        "two-factor",
        "2-factor",
        "MFA",
        "2FA",
        "authentication",
        "security"
    ],
    "homepage": "https://github.com/jacksonmg/laravel-mfa",
    "license": "MIT",
    "authors": [
        {
            "name": "David Shen",
            "email": "slj@slj.me",
            "homepage": "https://slj.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "^7.1|^8.0",
        "illuminate/support": "^5.7|^6|^7|^8|^9|^10"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "jacksonmg\\LaravelMFA\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jacksonmg\\LaravelMFA\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "jacksonmg\\LaravelMFA\\LaravelMFAServiceProvider"
            ]
        }
    }
}
