{
    "name": "sicaboy/laravel-mfa",
    "type": "library",
    "description": "A Laravel package of Multi-factor Authentication (MFA/2FA) with a middleware.",
    "keywords": [
        "laravel",
        "multi-factor",
        "two-factor",
        "2-factor",
        "MFA",
        "2FA",
        "authentication",
        "security"
    ],
    "homepage": "https://github.com/sicaboy/laravel-mfa",
    "license": "MIT",
    "authors": [
        {
            "name": "David Shen",
            "email": "slj@slj.me",
            "homepage": "http://slj.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.6|~7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Sicaboy\\LaravelMFA\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sicaboy\\LaravelMFA\\Test": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "Sicaboy\\LaravelMFA\\LaravelMFAServiceProvider"
            ]
        }
    }
}
